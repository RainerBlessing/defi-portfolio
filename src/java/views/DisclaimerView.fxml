<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="195.0" prefWidth="469.0" xmlns:fx="http://javafx.com/fxml/1" fx:controller="portfolio.views.DisclaimerView">
   <children>
      <ImageView fitHeight="95.0" fitWidth="99.0" layoutX="25.0" layoutY="25.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../icons/disclaimer.png" />
         </image>
      </ImageView>
      <Label fx:id="lblDisclaimer" layoutX="132.0" layoutY="25.0" prefHeight="95.0" prefWidth="330.0" text="The content provided is for informational purposes only." wrapText="true" />
      <Button fx:id="btnClose" layoutX="295.0" layoutY="127.0" mnemonicParsing="false" onAction="#btnClose" prefHeight="25.0" prefWidth="74.0" text="Close" />
      <CheckBox fx:id="hCheckBox" layoutX="133.0" layoutY="129.0" mnemonicParsing="false" text="Do not show again" />
   </children>
</AnchorPane>
